    "lodash": "^4.17.4",
    "markdown-loader": "^2.0.1",
    "marko": "^4.7.4",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "bootstrap-sass": "^3.3.7",
