{
  "name": "isomorphic-marko",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node --max-old-space-size=7168 ./tools/run start",
    "start:dist": "node ./build/server.js",
    "clean": "babel-node ./tools/run clean",
    "build": "cross-env NODE_ENV=production babel-node --max-old-space-size=7168 ./tools/run build",
    "build:client": "cross-env NODE_ENV=production babel-node --max-old-space-size=7168 ./tools/run build-client",
    "build:server": "cross-env NODE_ENV=production babel-node --max-old-space-size=7168 ./tools/run build-server",
    "analyse": "babel-node ./tools/analyse",
    "analyse:client": "webpack-bundle-analyzer build/webpack-client-stats.json build/public",
    "analyse:server": "webpack-bundle-analyzer build/webpack-server-stats.json",
    "test": "mocha \"src/**/*.test.js\" --require babel-register --require tests/setup.js",
    "eslint": "eslint \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wujjpp/isomorphic-marko.git"
  },
  "keywords": [
    "marko",
    "starter kit",
    "universal web",
    "webpack",
    "express",
    "babel",
    "es6",
    "template engine"
  ],
  "author": "Wu Jian Ping <wujjpp@hotmail.com>",
  "maintainers": [
    "Wu Jian Ping <wujjpp@hotmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wujjpp/isomorphic-marko/issues"
  },
